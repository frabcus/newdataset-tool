#!/usr/bin/env coffee
{exec} = require 'child_process'

process.chdir '/home/newdataset'

exec './install-extras.sh', (err, stdout, stderr) ->
  console.log """
  <script>
    $('content').html('<p class="loading">Loading tool</p>')
    window.content.trigger('tool:installed')
  </script>
  """
  process.exit 0
