#!/usr/bin/env coffee
{exec} = require 'child_process'

exec './install-extras.sh', (err, stdout, stderr) ->
  console.log """
  Please rename your dataset!!!!
  <script>
    console.log("From your tool: #{err}, #{stdout}, #{stderr}")
    window.content.trigger('tool:installed')
  </script>

  """
  process.exit 0
